#include<stdio.h>
#include<stdlib.h>
int f(char ch[],int c)
   {
     int i=0,j=0,k=1;
     int num=0;
     for (i=0;ch[i]!='\0';i++)
        {
          if (ch[i]==c)
              j++;
        }
     for (i=0;i<j;i++)
        {
          if (i==0)
            num=c;
          else 
            k=k*10;
            num=num+c*k;
        }
       
